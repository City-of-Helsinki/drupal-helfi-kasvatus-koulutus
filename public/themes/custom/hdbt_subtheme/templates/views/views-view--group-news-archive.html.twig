{% embed '@hdbt/component/content-list-with-count.twig' with {
  title: 'Latest news'|t({}, {'context' : 'Group news title'}),
  }%}
  {% block count_container%}
    {%- if total_rows -%}
      {{ total_rows }} {% trans with {'context': 'Group news count'}%}news{% plural total_rows %}news{% endtrans %}
    {%- else -%}
      {{ 'No results'|t({}, {'context' : 'Unit search no results title'}) }}
    {%- endif -%}
  {% endblock count_container %}
{% endembed %}
