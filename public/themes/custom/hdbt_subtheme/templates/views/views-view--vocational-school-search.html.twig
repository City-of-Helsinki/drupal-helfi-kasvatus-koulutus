{#
/**
 * @file
 * Theme override for main view template.
 */
#}

{% embed '@hdbt/component/unit-search.twig' with { 
  modifier_class: [
    'views--unit-search',
  ]
}%}
  {% block count_container%}
    {%- if total_rows -%}
      {{ total_rows }} {% trans with {'context': 'Vocational school search count'}%}unit{% plural total_rows %}units{% endtrans %}
    {%- else -%}
      {{ 'No results'|t({}, {'context' : 'Unit search no results title'}) }}
    {%- endif -%}
  {% endblock count_container %}
{% endembed %}
